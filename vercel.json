{
  "version": 2,
  "buildCommand": "cd apps/client && npm install && npm run build",
  "outputDirectory": "apps/client/.next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "cd apps/client && npm run dev",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}

